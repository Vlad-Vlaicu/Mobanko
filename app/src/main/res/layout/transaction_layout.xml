<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="21dp"
            app:srcCompat="@drawable/buttoncustom" />

        <TextView
            android:id="@+id/suma_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Suma tranzactionata: "
            android:textColor="#000000" />

        <TextView
            android:id="@+id/currency_transaction_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Moneda: "
            android:textColor="#000000" />

        <TextView
            android:id="@+id/data_transaction_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Data efectuarii tranzactiei: "
            android:textColor="#000000" />

        <TextView
            android:id="@+id/beneficiar_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Beneficiar: "
            android:textColor="#000000" />

        <TextView
            android:id="@+id/txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nota plata"
            android:textColor="#000000" />

        <EditText
            android:id="@+id/pay_note_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:minHeight="48dp"
            android:textColor="#000000"
            tools:ignore="SpeakableTextPresentCheck" />

    </LinearLayout>
</androidx.appcompat.widget.LinearLayoutCompat>